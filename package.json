{
  "name": "ii-admin-ui",
  "version": "1.2.0",
  "private": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "dist/index.d.ts",
  "description": "A library of react components, which is the landing of the design specifications of  AI-Indeeded Company.",
  "author": "ShiMu",
  "license": "MIT",
  "keywords": [
    "React",
    "Component"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "homepage": "https://lagrangelabs.github.io/ii-admin-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/LagrangeLabs/ii-admin-ui.git"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "changelog-A": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "changelog": "node ./scripts/print-changelog",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "@ant-design/icons": ">=4.2.0",
    "antd": ">=4.7.0",
    "classnames": ">=2.2.0",
    "react": ">=16.8.0"
  },
  "resolutions": {
    "@types/react": "^16.14.2",
    "mathjax-full": "3.1.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.7.3",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.3.1",
    "dumi": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "father-build": "^1.17.2",
    "fs-extra": "^9.1.0",
    "gh-pages": "^3.0.0",
    "inquirer": "^8.0.0",
    "jquery": "^3.6.0",
    "jsdom": "^16.5.2",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "open": "^8.0.4",
    "prettier": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "simple-git": "^2.37.0",
    "yorkie": "^2.0.0"
  }
}
